msgid "user-report.title"
msgstr "ኮድ መረጋገጺ ዉሰድ"

msgid "user-report.navbar"
msgstr "ኮድ መረጋገጺ ናይ ምቕላዕ ምልክታ ሕተት"

msgid "user-report.request-code-from"
msgstr "ኮድ መረጋገጺ ዉሰድ"

msgid "user-report.instructions"
msgstr "ን COVID-19 ኣወንታዊ መርመራ ምስ ዝህልወካ ግናኸ ብመልእኽቲ ወይ ብተሌፎን ካብ ትካል ጥዕናኻ ኮድ እንተ ዘይረኺብካ፣ ኣብዚ ክትሓትት ትኽእል ኢኻ።"

msgid "user-report.enter-date-instructions"
msgstr "ናይ COVID-19 ምርመራ ዝገበርካሉ ዕለት ኣእቱ"

msgid "user-report.phone-number-header"
msgstr "ናይ ቴሌፎን ቊጽርኻ"

msgid "user-report.phone-number-instructions"
msgstr "ናይ መረጋገጺ ኮድ ንምቕባል ቕቡል ዝኾነ ቊጽሪ ተሌፎን ከተእቱ ኣለካ። ናይ ስራሕ ክፍሊታት ክሕተቱ እኽእሉ እዮም።"

msgid "user-report.agreement-header"
msgstr "ስምምዕ"

msgid "user-report.agreement"
msgstr "ን COVID-19 ፖዘቲቭ ውጽኢት ፈተና ኸም ዝረኸብኩ ከምኡ’ውን እቲ ዘቕረብክዎ ቊጽሪ ተሌፎን ናተይ ምዃኑን ኣፍልጦ እህብ እየ፣ ብዛዕባ እቲ ዝረኸብክዎ ኣወንታዊ ውጽኢት ዝገልጽ ኮድ ድማ ክቕበል እደሊ እየ።"

msgid "user-report.request-button"
msgstr "ቀጽል"

msgid "user-report.code-issued"
msgstr "መረጋገጺ ኮድ ተፈቕደ"

msgid "user-report.check-mobile-device"
msgstr "ብኽብረትካ ነቲ ኣብ ሞባይልካ ዘሎ ብሞባይል ዝለኣኽ መልእኽቲ ርኣዮ።"

msgid "user-report.invalid-request"
msgstr "ዘይቕቡል ሕቶ፣ ብኽብረትካ ካብቲ ኣብ ሞባይልካ ዝርከብ ምቕላዕ ምልክታ መተግበሪ ስደድ።"

msgid "user-report.not-available"
msgstr "ኣብቲ ኸባቢካ ዘሎ ናይ ጥዕና ብዓል-ስልጣን ነቲ ኣብዚ እዋን እዚ ካብ ተጠቀምቲ ዘበገስዎ ጸብጻብ ኣይቅበሎን ኢዩ ዘሎ።"

msgid "user-report.missing-agreement"
msgstr "መረጋገጺ ኮድ ንምሕታት ነቲ ዅነታት ክትሰማምዓሉ ኣሎካ"

msgid "user-report.error-invalid-date"
